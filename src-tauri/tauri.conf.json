{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "S3Deck",
  "version": "0.3.1",
  "identifier": "com.odudek.s3deck",
  "build": {
    "devUrl": "http://localhost:1420",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "S3Deck",
        "width": 1000,
        "height": 600,
        "dragDropEnabled": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [],
    "macOS": {
      "signingIdentity": null,
      "entitlements": null
    }
  }
}
